#include<stdio.h>

void main(){
	float precio;
	float descuento;
	float precioFinal;
	
	printf("Ingrese el valor del producto ");
	scanf("%f",&precio);
	printf("Ingrese el descuento para el producto ");
	scanf("%f",&descuento);
	precioFinal=precio-precio*descuento/100;	
	
	printf("El precio final del producto es de %f",precioFinal);
}
